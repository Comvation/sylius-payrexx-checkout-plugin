{
    "name": "comvation/sylius-payrexx-checkout-plugin",
    "type": "sylius-plugin",
    "description": "Sylius Payrexx Checkout Plugin",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "license": "AGPL",
    "require": {
        "php": "^8.2",
        "sylius/sylius": "^1.11",
        "payrexx/payrexx": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.10-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Comvation\\SyliusPayrexxCheckoutPlugin\\": "src/",
            "Tests\\Comvation\\SyliusPayrexxCheckoutPlugin\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ]
    }
}
